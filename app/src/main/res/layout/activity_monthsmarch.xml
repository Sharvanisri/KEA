<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.hp.kea.Monthsmarch"
    android:background="@drawable/background">

    <ImageView
        android:layout_width="380dp"
        android:layout_height="300dp"
        android:id="@+id/imageView31"
        android:layout_gravity="center_horizontal|top"
        android:background="@drawable/march"/>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="125dp"
        android:layout_height="240dp"
        android:layout_gravity="left|bottom">

        <ImageButton
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:id="@+id/Marchpreviousbutton"
            android:background="@drawable/previousbutton"/>
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="125dp"
        android:layout_height="240dp"
        android:layout_gravity="center_horizontal|bottom">

        <ImageButton
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:id="@+id/Marchhomebutton"
            android:background="@drawable/homebutton"/>

        <ImageButton
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:id="@+id/Marchaudiobutton"
            android:background="@drawable/audiobutton"/>
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="125dp"
        android:layout_height="240dp"
        android:layout_gravity="right|bottom">

        <ImageButton
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:id="@+id/Marchnextbutton"
            android:background="@drawable/nextbutton"/>
    </LinearLayout>


</FrameLayout>
